{
  "name": "@wshops/utils",
  "author": "Tony An <anxuanzi@w-shops.com>",
  "homepage": "https://www.w-shops.com/",
  "description": "Wshops app frontend development toolkit",
  "private": false,
  "repository": {
    "url": "https://github.com/wshops/wshops-fe-utils.git"
  },
  "license": "MIT",
  "version": "1.0.8",
  "packageManager": "yarn@1.22.19",
  "main": "dist/index.cjs.js",
  "module": "dist/index.mjs.js",
  "unpkg": "dist/index.iife.js",
  "jsdelivr": "dist/index.iife.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "exports": {
    ".": {
      "require": "./dist/wshop.cjs",
      "import": "./dist/wshop.mjs"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "scripts": {
    "dev": "vite --host",
    "build": "tsc && vite build && dts-bundle-generator --config ./dts-bundle-generator.config.ts"
  },
  "devDependencies": {
    "@types/crypto-js": "^4.1.1",
    "@types/jsdom": "^21.1.0",
    "@types/node": "^18.13.0",
    "@types/qs": "^6.9.7",
    "axios": "^1.3.2",
    "crypto-js": "^4.1.1",
    "dts-bundle-generator": "^7.2.0",
    "js-base64": "^3.7.5",
    "less": "^4.1.3",
    "qs": "^6.11.0",
    "terser": "^5.16.3",
    "ts-md5": "^1.3.1",
    "typescript": "^4.9.5",
    "vite": "^4.1.1",
    "vite-plugin-banner": "^0.7.0",
    "vite-plugin-dts": "^1.7.2"
  }
}
